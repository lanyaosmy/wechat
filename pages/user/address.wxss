/* pages/user/address.wxss */
.contain{
  margin: 10rpx;
  height:80rpx;
  border:1rpx solid #aaa;
  border-radius: 10rpx;
  display: flex;
  align-items: center;
}
.on{
  border:1rpx solid #AAE6CC;
}
/*输入框*/
.inputBox{
  display: inline-block;
  margin-left: 20rpx;
  width: 78%;
  font-size: 30rpx;
}

.infoicon{
  width:40rpx;
  margin-left: 20rpx;
  height:40rpx;
}

picker{
  display: block;
}
.picker{
  font-size: 30rpx;
  border-bottom:1rpx solid #aaa;
  width: 450rpx;
}

button{
  width:200rpx;
  font-size: 30rpx;
  background:#AAE6CC;
  color: #FFF;
  border-radius: 50rpx;
}

.containnew{
  margin: 10rpx;
  height:200rpx;
  border:1rpx solid #aaa;
  border-radius: 10rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.inputline{
  width: 600rpx;
  height:80rpx;
  display: flex;
  align-items: center;
}
.inputline text{
  font-size: 25rpx;
  width: 100rpx;
  text-align: right;
  padding-right: 20rpx;
}

.inputline input{
  line-height:30rpx;
  border-bottom:1rpx solid #aaa;
  width: 500rpx;
  margin-top:10rpx;
  margin-bottom:10rpx;
}